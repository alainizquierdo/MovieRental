<Window x:Class="RentalsClient.InsertMovie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RentalsClient"
        mc:Ignorable="d"
        Title="InsertMovie" Height="115" Width="192">
    <Grid>
        <TextBox Text="{Binding title, Mode=TwoWay}" HorizontalAlignment="Center" Margin="0,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="36,51,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
